    "build:bundle": "node ./scripts/build.js",
    "build:types": "tsc",
    "build": "npm run build:bundle && npm run build:types"
  },
  "keywords": [
    "solid-js"
  ],
  "author": "Artemiy Schukin",
  "license": "MIT",
  "dependencies": {
    "clsx": "^1.2.1",
    "solid-js": "^1.4.7"
  },
  "devDependencies": {
    "@vanilla-extract/css": "^1.7.2",
    "@vanilla-extract/esbuild-plugin": "^2.1.0",
    "esbuild": "^0.14.49",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "solid-js": "^1.4.7",
    "@vanilla-extract/css": "^1.7.2"
  },
  "main": "./dist/index.jsx",
  "module": "./dist/index.jsx",
